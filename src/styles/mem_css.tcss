#mem-container {
    layout: grid;
    grid-size: 2;
    background: $panel;
    border-title-color: $text;
    border-title-align: center;
    border: yellow;
}

#mem-container:light {
    border: #F3CD00;
    background: $boost-darken-1;
}

.mem-static {
    height: 1fr;
    content-align: center middle;
}
